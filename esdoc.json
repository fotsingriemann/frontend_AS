{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.(js|jsx)$"],
  "outputAST": false,
  "plugins": [
    { "name": "esdoc-ecmascript-proposal-plugin", "option": { "all": true } },
    { "name": "esdoc-jsx-plugin", "option": { "enable": true } },
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": { "enable": true },
        "unexportedIdentifier": { "enable": true }
      }
    }
  ]
}
